{
    "name": "sinsquare/silex-bundle",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "~5.6|~7.0",
        "silex/silex": "^2.0",
        "symfony/finder": "~2.8|^3.0",
        "knplabs/console-service-provider": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": { "SinSquare\\Bundle\\": "src/Sins/Bundle/" }
    },
    "autoload-dev": {
        "psr-4": { "SinSquare\\Bundle\\Tests\\": "tests/Sins/Bundle/Tests/" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}